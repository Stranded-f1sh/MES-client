    <add key="Host" value="47.97.117.253:8092" />
    <add key="Referer" value="http://47.97.117.253:8092/" />
    <add key="User-Agent" value="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/80.0.3987.149 Safari/537.36" />
    <add key="Host" value="47.97.117.253:8092" />
    <add key="Host" value="47.97.117.253:8092" />
